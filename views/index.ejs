<%- include("partials/header") -%>
<!-- Current Weather -->
    <div class="mainbox">
      <form action="/" method="post">
        <input type="text" name="City" placeholder="Search by City or Zip Code" autocomplete="off" class="form-control search-box">
        <button class="btn btn-default" type="submit">
          <i class="fas fa-search fa-lg"></i>
        </button>
      </form>
      <h1 class="location"> <%= SendData.location%></h1>
      <h1 id="m1"><%= SendData.currTemp%></p>
      <img src="<%= SendData.currIcon%>">
      <h2 id="m2"><%= SendData.currDesc%></p>
      <h2><%= SendData.currHumidity %></h2>
      <h2><%= SendData.currTempMin %></h2>
      <h2><%= SendData.currTempMax %></h2>
      <h2><%= SendData.currWindSpeed %></h2>
    </div>



    <%- include("partials/footer") -%>
